{
  "name": "reactgames",
  "version": "1.0.0",
  "description": "Simple fun Games",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Strategic Machines",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.2",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.21",
    "mdbootstrap": "^4.15.0",
    "mongoose": "5.7.5",
    "morgan": "^1.9.1",
    "npm": "^6.5.0",
    "react-detect-offline": "^2.4.0",
    "react-table-v6": "^6.8.6",
    "react-touch-events": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.7"
  }
}
